{
	"name": "learnpress",
	"version": "1.0.0",
	"description": "[![Stories in Ready](https://badge.waffle.io/LearnPress/LearnPress.svg?label=ready&title=Ready)](http://waffle.io/LearnPress/LearnPress)",
	"scripts": {
		"test": "./vendor/bin/phpcs -d date.timezone=UTC",
		"dev": "wp-scripts start --progress",
		"dev-watch": "cross-env BABEL_ENV=default webpack --mode=development --watch",
		"dev-packages": "cross-env BABEL_ENV=default webpack --config tools/build.packages.js --mode=development --watch",
		"build-packages": "cross-env BABEL_ENV=default NODE_ENV=production webpack --config tools/build.packages.js --progress -p",
		"build": "cross-env BABEL_ENV=default NODE_ENV=production webpack --progress -p",
		"prod": "webpack --progress --colors -p --config webpack.config.js --mode=production",
		"dev-build": "npm run build && npm run build-packages && gulp styles && npm run dev && npm run dev-packages",
		"release": "npm run build && npm run build-packages && gulp release",
		"babel": "cross-env NODE_ENV=production babel assets/src/apps/js",
		"makepot": "wp i18n make-pot . --skip-audit --exclude=\"test,releases,build,tools,dist,node_modules,vendor,wordpress,\" --headers='{\"last-translator\":\"admin@email.com\",\"Report-Msgid-Bugs-To\":\"https://github.com/LearnPress/learnpress/issues\"}' languages/learnpress.pot"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LearnPress/LearnPress.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/LearnPress/LearnPress/issues"
	},
	"homepage": "https://github.com/LearnPress/LearnPress#readme",
	"dependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"classnames": "^2.3.1",
		"jquery": "^3.6.0",
		"refx": "^3.1.1",
		"sortablejs": "^1.13.0"
	},
	"devDependencies": {
		"@wordpress/babel-plugin-makepot": "^4.2.0",
		"@wordpress/scripts": "^19.2.2",
		"babel-loader": "^8.2.3",
		"del": "^6.0.0",
		"eslint": "^7.28.0",
		"gulp": "^4.0.2",
		"gulp-cache": "^1.1.3",
		"gulp-cli": "^2.3.0",
		"gulp-line-ending-corrector": "^1.0.3",
		"gulp-notify": "^3.2.0",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^9.0.0",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^0.5.4",
		"gulp-sass": "^5.0.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-uglify": "^3.0.2",
		"gulp-uglify-es": "^2.0.0",
		"gulp-uglifycss": "^1.1.0",
		"gulp-vinyl-zip": "^2.2.1",
		"gulp-wp-pot": "^2.5.0",
		"read-file": "^0.2.0",
		"uglify-js": "^3.14.4",
		"uglifycss": "0.0.29",
		"webpack-merge-and-include-globally": "^2.3.4"
	}
}
